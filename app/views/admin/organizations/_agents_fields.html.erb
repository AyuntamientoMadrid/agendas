<div id="<%= dom_id(f.object) %>" class="nested-fields">
  <%= f.hidden_field :id %>

  <%= f.label :identifier %>
  <%= f.text_field :identifier %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :from %>
  <%= f.date_field :from %>

  <%= f.label :to %>
  <%= f.date_field :to %>

  <%= f.label :public_assignments %>
  <%= f.text_area :public_assignments %>
  <small>Solo rellenar el campo en caso de cumplir la pregunta.</small>

  <%= link_to_remove_association "Eliminar", f %>
</div>
