<div id="<%= dom_id(f.object) %>" class="nested-fields">
  <%= f.hidden_field :id %>
  <%= f.hidden_field :role, value: :lobby %>
  <%= f.hidden_field :active, value: 1 %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :phones %>
  <%= f.text_field :phones %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>  

</div>
